
@{
    ViewBag.Title = "TipoClienteNuevo";
}

<h2>TipoClienteNuevo</h2>

<form id="frmNuevoTipoCliente" method="post">
    <div class="form-group">
        <label>Nombre:</label>
        <input type="text" id="Nombre" name="Nombre" class="form-control" />
    </div>
   
 
    <div class="form-group">
        <label>Descripcion:</label>
        <input type="text" name="Descripcion" id="Descripcion" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>


    <a href="~/TiposClientes/TipoClienteLista">Regresar a la Lista</a>


</form>

@Html.ValidationSummary(true)
@section scripts{
    <script src="~/Scripts/Validaciones/ClienteNuevo.js"></script>
}