@model ProyectoFinal.Models.sp_RetornaTipos_Cuentas_Result

@{
    ViewBag.Title = "TipoCuentaModifica";
}

<h2>Tipo de Cuentas Modifica</h2>

<form id="frmModificaTipoCuentas" method="post">
    <div class="form-group">
        <label>Nombre:</label>
        <input type="text" id="Nombre" name="Nombre" class="form-control" value="@Model.Nombre" />
    </div>

    <div class="form-group">
        <label>Codigo:</label>
        <input type="text" name="Codigo" id="Codigo" class="form-control" value="@Model.Codigo" />
    </div>

    <div class="form-group">
        <input type="submit" value="Modificar" id="btnAceptar" />
    </div>


    <a href="~/TipoCuentas/TipoCuentasLista">Regresar a la Lista</a>


</form>

@Html.ValidationSummary(true)
@section scripts{
    <script src="~/Scripts/Validaciones/ClienteNuevo.js"></script>
}