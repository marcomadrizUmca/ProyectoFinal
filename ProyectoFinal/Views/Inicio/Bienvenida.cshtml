@model ProyectoFinal.Models.sp_AutenticarUsuario_Result
@{
    ViewBag.Title = "Bienvenida";
}

<div>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-sm-1"></label>
            <div class="col-sm-7">
                <h2>Bienvenido(a)</h2>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-4" for="nombrecompleto">Nombre Completo:</label>
            <div class="col-sm-4">
                @Html.DisplayFor(model => model.nombrecompleto)
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-4" for="ultimoIngreso">Fecha de Ingreso</label>
            <div class="col-sm-4">
                @Html.DisplayFor(model => model.ultimoIngreso)
            </div>
        </div>


    </form>
</div>

