@model ProyectoFinal.Models.sp_AutenticarUsuario_Result
@{
    ViewBag.Title = "Bienvenida";
}

<div>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-sm-1"></label>
            <div class="col-sm-7">
                <h2>Bienvenido(a)</h2>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-4" for="nombrecompleto">Nombre Completo:</label>
            <div class="col-sm-4">
                @Html.DisplayFor(model => model.nombrecompleto)
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-4" for="ultimoIngreso">Fecha de Ingreso</label>
            <div class="col-sm-4">
                @Html.DisplayFor(model => model.ultimoIngreso)
            </div>
        </div>


    </form>
    

</div>
<div class="col-sm-4">
    <h3>Ver Video</h3>
    

    <video width="320" height="240" controls="controls">
        <source src="~/Video/Banco%20del%20Pueblo%20Soberano%20-%20Organizando%20las%20fuerzas%20productivas.mp4" type="video/mp4" />
        <source src="~/Video/Banco%20del%20Pueblo%20Soberano%20-%20Organizando%20las%20fuerzas%20productivas.ogg" type="video/ogg" />
        El navegador no soporta video.
    </video>
    <div/>
    <div class="col-sm-4">
        <h3>Audio</h3>
        <audio controls>
            <a href="~/Audio/videoplayback.m4a">~/Audio/videoplayback.m4a</a>
    </audio>
        <h3>BANCO DEL PUEBLO</h3>
        <figure>
            <img src="~/Imagenes/banco del pueblo.PNG"  />
            <figcaption>Somos del Pueblo y para el Pueblo</figcaption>
        </figure>
        <div/>


    </div>


   


</div>

